{
  "name": "near-chess",
  "description": "",
  "version": "0.0.1",
  "scripts": {
    "build": "mkdir -p out/ && gulp",
    "dev:deploy:contract": "near dev-deploy ./out/main.wasm",
    "deploy:contract": "near deploy",
    "deploy:pages": "gh-pages -d src",
    "deploy": "npm run build && npm run deploy:contract && npm run deploy:pages",
    "prestart": "npm run build && npm run dev:deploy:contract",
    "start": "CONTRACT_NAME=$(cat neardev/dev-account) parcel src/index.html",
    "dev": "nodemon --watch assembly --exec 'npm run start'",
    "test": "npm run build && jest test --env=near-shell/test_environment"
  },
  "devDependencies": {
    "bignum": "github:MaxGraey/bignum.wasm",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.2",
    "jest": "^22.4.4",
    "jest-environment-node": "^24.5.0",
    "near-runtime-ts": "^0.5.1",
    "near-shell": "0.20.1-preview",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4"
  },
  "wasmStudio": {
    "name": "NEAR Chess",
    "description": "Shows example of how to implement on-chain chess game and deploy to GitHub pages",
    "icon": "typescript-lang-file-icon"
  },
  "dependencies": {
    "near-bindgen-as": "^1.2.0",
    "nearlib": "^0.20.0",
    "regenerator-runtime": "^0.13.3",
    "serve": "^11.0.1"
  }
}
